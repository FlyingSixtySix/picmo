{
  "name": "emoji-button",
  "version": "5.0.0",
  "description": "Vanilla JavaScript emoji picker",
  "keywords": [
    "emoji",
    "javascript"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "clean": "rimraf dist",
    "start": "concurrently -n \"type-check,serve\" -c \"blueBright,yellowBright\" \"npm:check:watch\" \"npm:serve\"",
    "test": "jest src/__tests__/**.test.ts",
    "test:watch": "jest --watchAll",
    "lint": "eslint src/*.ts",
    "prettify": "prettier src/*.ts --write",
    "serve": "parcel dev/index.html",
    "storybook": "start-storybook -p 6006",
    "update-scss-types": "typed-scss-modules src",
    "build-storybook": "build-storybook"
  },
  "author": "Joe Attardi <jattardi@gmail.com> (https://joeattardi.dev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/joeattardi/emoji-button.git"
  },
  "homepage": "https://emoji-button.js.org",
  "bugs": "https://github.com/joeattardi/emoji-button/issues",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-inline-string": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addons": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/html": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/theming": "^6.4.19",
    "@types/ejs": "^3.1.0",
    "@types/jest": "^25.2.3",
    "@types/twemoji": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.0.1",
    "parcel": "^2.0.1",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.6",
    "typed-scss-modules": "^6.2.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@popperjs/core": "^2.4.0",
    "ejs": "^3.1.6",
    "emojibase": "^6.1.0",
    "escape-html": "^1.0.3",
    "focus-trap": "^5.1.0",
    "tiny-emitter": "^2.1.0",
    "tslib": "^2.0.0",
    "twemoji": "^12.1.2"
  },
  "files": [
    "dist"
  ]
}
